### File managed with puppet ###
## Module:           'echoes_alert'
## Template source:  'MODULES/echoes_alert/templates/gui/etc/default/ea-gui.erb'

#General options:
EA_GUI_SERVERNAME=<%= servername %> # Servername (IP address or DNS name)
EA_GUI_ACCESS_LOG=<%= log_dir %>/gui_access.log

# HTTP server options:
EA_GUI_IFACE=0.0.0.0 # IPv4 (e.g. 0.0.0.0) or IPv6 Address (e.g. 0::0)
EA_GUI_PORT=<%= port %> # HTTP port (e.g. 80)

# HTTPS server options:
EA_GUI_HTTPS_IFACE=0.0.0.0 # IPv4 (e.g. 0.0.0.0) or IPv6 Address (e.g. 0::0)
EA_GUI_HTTPS_PORT=<%= ssl_port %> # HTTPS port (e.g. 443)
EA_GUI_SSL_CERT=/etc/ssl/echoes-tech.com/bundle-echoes-tech.com.crt # SSL server certificate chain file
EA_GUI_SSL_PRIV_KEY=/etc/ssl/echoes-tech.com/echoes-tech.com.key # SSL server private key file
