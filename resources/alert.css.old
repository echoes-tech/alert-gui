body {
font-family: Arial,sans-serif;
}

table {
border: thin solid #AACFE4;
border-collapse: collapse;
margin-top: 20px;
}
.table-list th {
border: thin solid #AACFE4;
width: auto;
padding: 5px;
background-color: #D0E3FA;
}
.table-list td {
border: thin solid #AACFE4;
width: auto;
padding: 5px;
text-align: center;
background-color: #ffffff;
}


.Wt-tabs ul {
    padding: 3px 3px 0px;
    font: bold 14px Arial,sans-serif;
    list-style-type: none;
    margin: 0px;
}